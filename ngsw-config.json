{
   "index": "/index.html",
   "assetGroups": [
      {
         "name": "app",
         "installMode": "prefetch",
         "resources": {
            "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
         }
      },
      {
         "name": "assets",
         "installMode": "lazy",
         "updateMode": "prefetch",
         "resources": {
            "files": ["/assets/**"],
            "urls": ["https://fonts.googleapis.com/**", "https://ajax.googleapis.com/**"]
         }
      }
   ],
   "dataGroups": [
      {
         "name": "api",
         "urls": ["/graphql"],
         "cacheConfig": {
            "maxSize": 0,
            "maxAge": "0u",
            "strategy": "freshness"
         }
      }
   ]
}
