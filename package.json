{
   "name": "portfolio",
   "version": "0.0.6",
   "engines": {
      "node": "^8.10.0",
      "npm": ">=5.0.0",
      "yarn": ">=1.3.0"
   },
   "license": "MIT",
   "repository": {
      "type": "git",
      "url": "https://github.com/samuelfernandez/portfolio"
   },
   "scripts": {
      "precommit": "lint-staged",
      "start": "ng serve",
      "build": "npm run build:client && npm run build:server",
      "build:client": "ng build --prod --build-optimizer --progress false",
      "build:server": "webpack --config webpack.server.config.js --colors",
      "test": "ng test",
      "lint": "gulp lint",
      "e2e": "ng e2e",
      "server": "node server.js",
      "ci:test": "ng test --watch=false --browsers ChromeNoSandbox",
      "ci:e2e": "ng e2e --protractor-config=e2e/protractor-ci.conf.js --prod",
      "ci:e2e-remote": "ng e2e --protractor-config=e2e/protractor-ci.conf.js --dev-server-target= --base-url=$URL",
      "ci:audit-pagespeed": "gulp pagespeed --url=$URL --threshold=$PAGESPEED_THRESHOLD",
      "ci:audit-lighthouse": "gulp lighthouse --url=$URL --threshold=$LIGHTHOUSE_THRESHOLD --runs 3",
      "docker:login": "docker login -u $DOCKER_USER --password-stdin",
      "docker:build": "docker build -t $DOCKER_APP .",
      "docker:run": "docker run --rm -it -p 5000:5000 -e PORT=5000 $DOCKER_APP",
      "docker:local": "docker run -it -p 5000:5000 -e PORT=5000 -v $(pwd)/dist:/usr/src/app:ro $DOCKER_APP sh",
      "docker:push": "docker push $DOCKER_APP",
      "heroku:login": "heroku container:login",
      "heroku:deploy": "heroku container:push web --app $APP_NAME",
      "heroku:release": "heroku container:release web --app $APP_NAME"
   },
   "main": "server.js",
   "dependencies": {
      "@angular/animations": "^6.0.7",
      "@angular/common": "^6.0.7",
      "@angular/compiler": "^6.0.7",
      "@angular/core": "^6.0.7",
      "@angular/forms": "^6.0.7",
      "@angular/http": "^6.0.7",
      "@angular/platform-browser": "^6.0.7",
      "@angular/platform-browser-dynamic": "^6.0.7",
      "@angular/router": "^6.0.7",
      "core-js": "^2.5.4",
      "express": "^4.16.3",
      "rxjs": "^6.0.0",
      "zone.js": "^0.8.26"
   },
   "devDependencies": {
      "@angular-devkit/build-angular": "~0.6.8",
      "@angular/cli": "~6.0.8",
      "@angular/compiler-cli": "^6.0.7",
      "@angular/language-service": "^6.0.7",
      "@types/jasmine": "~2.8.6",
      "@types/jasminewd2": "~2.0.3",
      "@types/node": "~10.5.1",
      "bluebird": "^3.5.1",
      "chalk": "^2.4.1",
      "chrome-launcher": "^0.10.2",
      "codelyzer": "~4.4.2",
      "eslint": "^5.0.1",
      "eslint-config-prettier": "^2.9.0",
      "eslint-plugin-import": "^2.13.0",
      "eslint-plugin-node": "^7.0.0",
      "eslint-plugin-prettier": "^2.6.1",
      "eslint-plugin-unicorn": "^4.0.3",
      "gulp": "^4.0.0",
      "gulp-run-command": "^0.0.9",
      "htmllint-cli": "^0.0.7",
      "husky": "^0.14.3",
      "jasmine": "^3.1.0",
      "jasmine-core": "~3.1.0",
      "jasmine-spec-reporter": "~4.2.1",
      "js-beautify": "^1.7.5",
      "karma": "~2.0.4",
      "karma-chrome-launcher": "~2.2.0",
      "karma-coverage-istanbul-reporter": "~2.0.0",
      "karma-jasmine": "~1.1.1",
      "karma-jasmine-html-reporter": "^1.1.0",
      "lighthouse": "^3.0.0",
      "lint-staged": "^7.2.0",
      "octonode": "^0.9.2",
      "prettier": "^1.13.7",
      "protractor": "~5.3.0",
      "psi": "^3.1.0",
      "require-dir": "^1.0.0",
      "stylelint": "^9.3.0",
      "stylelint-config-prettier": "^3.3.0",
      "stylelint-config-standard": "^18.2.0",
      "ts-node": "~7.0.0",
      "tslint": "~5.10.0",
      "typescript": "~2.7.2",
      "webpack": "^4.14.0",
      "webpack-cli": "^3.0.8",
      "yargs": "^12.0.1"
   },
   "greenkeeper": {
      "ignore": [
         "@angular/animations",
         "@angular/common",
         "@angular/compiler",
         "@angular/core",
         "@angular/forms",
         "@angular/http",
         "@angular/platform-browser",
         "@angular/platform-browser-dynamic",
         "@angular/router",
         "rxjs",
         "zone.js",
         "@angular/compiler-cli",
         "@angular-devkit/build-angular",
         "typescript",
         "@angular/cli",
         "@angular/language-service",
         "@types/jasmine",
         "@types/jasminewd2",
         "@types/node",
         "jasmine-core",
         "jasmine-spec-reporter",
         "karma",
         "karma-chrome-launcher",
         "karma-coverage-istanbul-reporter",
         "karma-jasmine",
         "karma-jasmine-html-reporter",
         "protractor",
         "ts-node",
         "tslint"
      ]
   }
}
