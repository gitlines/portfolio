{
   "name": "portfolio",
   "version": "0.0.10",
   "description": "Portfolio using a full-stack web frontend based on Angular",
   "keywords": [
      "angular",
      "angular-cli",
      "angular6",
      "heroku",
      "docker",
      "docker-compose",
      "pagespeed",
      "lighthouse",
      "jest",
      "compodoc",
      "codacy",
      "codeclimate",
      "cucumber"
   ],
   "homepage": "https://samuel-fernandez.herokuapp.com/",
   "engines": {
      "node": "^8.10.0",
      "npm": ">=5.0.0",
      "yarn": ">=1.3.0"
   },
   "license": "MIT",
   "repository": {
      "type": "git",
      "url": "https://github.com/samuelfernandez/portfolio"
   },
   "scripts": {
      "precommit": "lint-staged",
      "start": "ng serve",
      "build": "yarn run build:client && yarn run build:server",
      "build:client": "ng build --prod --build-optimizer --progress false",
      "build:server": "webpack --config webpack.server.config.js",
      "test": "jest --watch",
      "test:update-snapshot": "jest --updateSnapshot",
      "test:ci": "jest --runInBand --coverage",
      "test:doc": "compodoc --coverageTest $COMPODOC_THRESHOLD",
      "lint": "gulp lint",
      "e2e": "protractor e2e/protractor.conf.js",
      "e2e:update-webdriver": "webdriver-manager update --gecko false",
      "e2e:remote": "yarn e2e --baseUrl=$URL",
      "server": "node server.js",
      "pagespeed": "gulp pagespeed --url=$URL --threshold=$PAGESPEED_THRESHOLD",
      "lighthouse": "gulp lighthouse --url=$URL --threshold=$LIGHTHOUSE_THRESHOLD --runs 3",
      "compodoc": "compodoc",
      "docker:login": "docker login -u $DOCKER_USER --password-stdin",
      "docker:build": "docker build -t samuelfernandez/portfolio .",
      "docker:run": "docker run --rm -it -p 5000:5000 -e PORT=5000 samuelfernandez/portfolio",
      "docker:local": "docker run -it -p 5000:5000 -e PORT=5000 -v $(pwd)/dist:/usr/src/app:ro samuelfernandez/portfolio sh",
      "docker:push": "docker push samuelfernandez/portfolio",
      "docker:compose": "yarn run build && docker-compose up",
      "heroku:login": "heroku container:login",
      "heroku:deploy": "heroku container:push web --app $APP_NAME",
      "heroku:release": "heroku container:release web --app $APP_NAME"
   },
   "main": "server.js",
   "dependencies": {
      "@angular/animations": "^6.1.0",
      "@angular/common": "^6.1.0",
      "@angular/compiler": "^6.1.0",
      "@angular/core": "^6.1.0",
      "@angular/forms": "^6.1.0",
      "@angular/http": "^6.1.0",
      "@angular/platform-browser": "^6.1.0",
      "@angular/platform-browser-dynamic": "^6.1.0",
      "@angular/router": "^6.1.0",
      "core-js": "^2.5.4",
      "express": "^4.16.3",
      "rxjs": "^6.0.0",
      "zone.js": "^0.8.26"
   },
   "devDependencies": {
      "@angular-devkit/build-angular": "~0.7.1",
      "@angular/cli": "~6.1.1",
      "@angular/compiler-cli": "^6.1.0",
      "@angular/language-service": "^6.1.0",
      "@compodoc/compodoc": "^1.1.3",
      "@types/chai": "^4.1.4",
      "@types/cucumber": "^4.0.4",
      "@types/jest": "^23.3.0",
      "@types/node": "~10.5.1",
      "bluebird": "^3.5.1",
      "chai": "^4.1.2",
      "chalk": "^2.4.1",
      "chrome-launcher": "^0.10.2",
      "codacy-coverage": "^3.0.0",
      "codelyzer": "~4.4.2",
      "cucumber": "^4.2.1",
      "cucumber-pretty": "^1.4.4",
      "eslint": "^5.0.1",
      "eslint-config-prettier": "^3.0.0",
      "eslint-plugin-import": "^2.13.0",
      "eslint-plugin-node": "^7.0.1",
      "eslint-plugin-prettier": "^3.0.0",
      "eslint-plugin-unicorn": "^6.0.0",
      "fs-extra": "^7.0.0",
      "gulp": "^4.0.0",
      "gulp-run-command": "^0.0.9",
      "htmllint-cli": "^0.0.7",
      "husky": "^1.1.2",
      "jest": "^22.4.4",
      "jest-preset-angular": "^5.2.3",
      "js-beautify": "^1.7.5",
      "lighthouse": "^3.0.0",
      "lint-staged": "^8.0.0",
      "nugget": "^2.0.1",
      "octonode": "^0.9.2",
      "prettier": "^1.13.7",
      "protractor": "^5.4.0",
      "protractor-cucumber-framework": "^6.0.0",
      "protractor-multiple-cucumber-html-reporter-plugin": "^1.7.1",
      "psi": "^3.1.0",
      "require-dir": "^1.0.0",
      "stylelint": "^9.3.0",
      "stylelint-config-prettier": "^4.0.0",
      "stylelint-config-standard": "^18.2.0",
      "ts-node": "~7.0.0",
      "tslint": "~5.11.0",
      "tslint-config-prettier": "^1.13.0",
      "typescript": "~2.9.2",
      "webpack": "^4.20.2",
      "webpack-cli": "^3.0.8",
      "yargs": "^12.0.1"
   },
   "greenkeeper": {
      "ignore": [
         "@angular/animations",
         "@angular/common",
         "@angular/compiler",
         "@angular/core",
         "@angular/forms",
         "@angular/http",
         "@angular/platform-browser",
         "@angular/platform-browser-dynamic",
         "@angular/router",
         "rxjs",
         "zone.js",
         "@angular/compiler-cli",
         "@angular-devkit/build-angular",
         "typescript",
         "@angular/cli",
         "@angular/language-service",
         "@types/node",
         "protractor",
         "ts-node",
         "tslint"
      ]
   }
}
