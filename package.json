{
   "name": "portfolio",
   "version": "0.0.0-development",
   "description": "Portfolio using a full-stack web frontend based on Angular",
   "keywords": [
      "angular",
      "angular-cli",
      "angular6",
      "heroku",
      "docker",
      "docker-compose",
      "pagespeed",
      "lighthouse",
      "jest",
      "compodoc",
      "codacy",
      "codeclimate",
      "cucumber",
      "app shell",
      "ssr",
      "server side rendering",
      "universal",
      "pwa"
   ],
   "homepage": "https://samuel-fernandez.herokuapp.com/",
   "engines": {
      "node": "^8.10.0",
      "npm": ">=6.0.0"
   },
   "license": "MIT",
   "repository": {
      "type": "git",
      "url": "https://github.com/samuelfernandez/portfolio.git"
   },
   "scripts": {
      "commit": "git-cz",
      "postinstall": "npm run e2e:update-webdriver",
      "start": "ng serve",
      "build": "npm run build:client && npm run build:server && npm run optimize",
      "build:client": "ng run portfolio:app-shell:production",
      "build:server": "webpack --config webpack.server.config.js",
      "optimize": "gulp optimize",
      "test": "jest --watch",
      "test:update-snapshot": "jest --updateSnapshot",
      "test:ci": "jest --runInBand --coverage",
      "test:doc": "compodoc --coverageTest $COMPODOC_THRESHOLD",
      "lint": "gulp lint",
      "e2e": "protractor e2e/protractor.conf.js",
      "e2e:update-webdriver": "webdriver-manager update --gecko false",
      "e2e:remote": "npm run e2e -- --baseUrl=$URL",
      "server": "node server.js",
      "pagespeed": "gulp pagespeed --url=$URL --threshold=$PAGESPEED_THRESHOLD",
      "lighthouse": "gulp lighthouse --url=$URL --threshold=$LIGHTHOUSE_THRESHOLD --runs 3",
      "compodoc": "compodoc",
      "docker:login": "docker login -u $DOCKER_USER --password-stdin",
      "docker:build": "docker build -t samuelfernandez/portfolio .",
      "docker:run": "docker run --rm -it -p 5000:5000 -e PORT=5000 samuelfernandez/portfolio",
      "docker:local": "docker run -it -p 5000:5000 -e PORT=5000 -v \"%cd%\"/dist:/usr/src/app:ro samuelfernandez/portfolio sh",
      "docker:push": "docker push samuelfernandez/portfolio",
      "docker:compose": "npm run build && docker-compose up",
      "heroku:login": "heroku container:login",
      "heroku:deploy": "heroku container:push web --app $APP_NAME",
      "heroku:release": "heroku container:release web --app $APP_NAME",
      "semantic-release": "semantic-release"
   },
   "main": "server.js",
   "dependencies": {
      "@angular/animations": "^7.0.4",
      "@angular/common": "^7.0.4",
      "@angular/compiler": "^7.0.4",
      "@angular/core": "^7.0.4",
      "@angular/forms": "^7.0.4",
      "@angular/http": "^7.0.4",
      "@angular/platform-browser": "^7.0.4",
      "@angular/platform-browser-dynamic": "^7.0.4",
      "@angular/platform-server": "^7.0.4",
      "@angular/pwa": "^0.10.6",
      "@angular/router": "^7.0.4",
      "@angular/service-worker": "^7.0.3",
      "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
      "angulartics2": "^7.1.0",
      "compression": "^1.7.3",
      "core-js": "^2.5.4",
      "express": "^4.16.3",
      "helmet": "^3.15.0",
      "heroku-ssl-redirect": "^0.0.4",
      "is-heroku": "^2.0.0",
      "morgan": "^1.9.1",
      "normalize.css": "^8.0.1",
      "rxjs": "^6.3.3",
      "serve-favicon": "^2.5.0",
      "ts-loader": "^5.3.0",
      "zone.js": "^0.8.26"
   },
   "devDependencies": {
      "@angular-devkit/build-angular": "~0.10.6",
      "@angular/cli": "~7.0.6",
      "@angular/compiler-cli": "^7.0.4",
      "@angular/language-service": "^7.0.4",
      "@commitlint/cli": "^7.2.1",
      "@commitlint/config-conventional": "^7.1.2",
      "@compodoc/compodoc": "^1.1.3",
      "@types/chai": "^4.1.4",
      "@types/cucumber": "^4.0.4",
      "@types/jest": "^23.3.0",
      "@types/node": "~10.12.8",
      "bluebird": "^3.5.1",
      "chai": "^4.1.2",
      "chalk": "^2.4.1",
      "chrome-launcher": "^0.10.2",
      "codacy-coverage": "^3.0.0",
      "codelyzer": "~4.5.0",
      "commitizen": "^3.0.4",
      "critical": "^1.3.4",
      "cssnano": "^4.1.7",
      "cssnano-preset-advanced": "^4.0.5",
      "cucumber": "^5.0.2",
      "cucumber-pretty": "^1.4.4",
      "cz-conventional-changelog": "^2.1.0",
      "eslint": "^5.0.1",
      "eslint-config-prettier": "^3.0.0",
      "eslint-plugin-import": "^2.13.0",
      "eslint-plugin-node": "^8.0.0",
      "eslint-plugin-prettier": "^3.0.0",
      "eslint-plugin-unicorn": "^6.0.0",
      "fs-extra": "^7.0.1",
      "gulp": "^4.0.0",
      "gulp-cheerio": "^0.6.3",
      "gulp-html-postcss": "^7.1.4",
      "gulp-imagemin": "^5.0.3",
      "gulp-inline-source": "^3.2.0",
      "gulp-postcss": "^8.0.0",
      "gulp-purifycss": "^0.2.0",
      "gulp-replace": "^1.0.0",
      "gulp-run-command": "^0.0.9",
      "htmllint-cli": "^0.0.7",
      "husky": "^1.1.4",
      "imagemin-mozjpeg": "^8.0.0",
      "jest": "^23.6.0",
      "jest-preset-angular": "^6.0.1",
      "js-beautify": "^1.7.5",
      "lighthouse": "^4.0.0-alpha.1",
      "lint-staged": "^8.0.0",
      "nugget": "^2.0.1",
      "octonode": "^0.9.2",
      "prettier": "^1.13.7",
      "protractor": "^5.4.0",
      "protractor-cucumber-framework": "^6.0.0",
      "protractor-multiple-cucumber-html-reporter-plugin": "^1.7.1",
      "psi": "^3.1.0",
      "require-dir": "^1.0.0",
      "rxjs-tslint": "^0.1.5",
      "semantic-release": "^15.12.0",
      "stylelint": "^9.3.0",
      "stylelint-config-prettier": "^4.0.0",
      "stylelint-config-standard": "^18.2.0",
      "ts-node": "~7.0.0",
      "tslint": "~5.11.0",
      "tslint-config-prettier": "^1.13.0",
      "typescript": "~3.1.6",
      "webpack-cli": "^3.1.2",
      "yargs": "^12.0.1"
   }
}
