{
   "name": "portfolio",
   "version": "0.0.0-development",
   "description": "Portfolio using a full-stack web frontend based on Angular",
   "keywords": [
      "angular",
      "angular-cli",
      "angular6",
      "heroku",
      "docker",
      "docker-compose",
      "pagespeed",
      "lighthouse",
      "jest",
      "compodoc",
      "codacy",
      "codeclimate",
      "cucumber",
      "app shell",
      "ssr",
      "server side rendering",
      "universal",
      "pwa",
      "graphql"
   ],
   "homepage": "https://samuel-fernandez.herokuapp.com/",
   "engines": {
      "node": "^8.10.0",
      "npm": ">=6.0.0"
   },
   "license": "MIT",
   "repository": {
      "type": "git",
      "url": "https://github.com/samuelfernandez/portfolio.git"
   },
   "scripts": {
      "commit": "git-cz",
      "postinstall": "npm run e2e:update-webdriver",
      "start": "ng serve",
      "build": "npm run build:client && npm run build:server",
      "build:client": "ng run portfolio:build:production && npm run optimize",
      "build:shell": "ng run portfolio:app-shell:production",
      "build:server": "ng run portfolio:server:production && webpack --config src/webpack.config.js --progress",
      "build:server:watch": "webpack --config src/webpack.config.js --progress --watch",
      "build:analyze": "webpack-bundle-analyzer dist/browser/stats.json",
      "optimize": "gulp optimize",
      "sitemap": "gulp sitemap --local-url http://localhost:5000/ --output-path dist/browser",
      "dotenv": "gulp dotenv",
      "test": "jest --watch",
      "test:update-snapshot": "jest --updateSnapshot",
      "test:ci": "jest --runInBand --coverage",
      "test:doc": "compodoc --coverageTest $COMPODOC_THRESHOLD",
      "lint": "gulp lint",
      "e2e": "protractor e2e/protractor.conf.js",
      "e2e:update-webdriver": "webdriver-manager update --gecko false",
      "e2e:remote": "npm run e2e -- --baseUrl=$HOST",
      "server": "node dist/server.js",
      "server:watch": "nodemon --inspect dist/server.js",
      "pagespeed": "gulp pagespeed --url=$HOST --threshold=$PAGESPEED_THRESHOLD",
      "lighthouse": "gulp lighthouse --url=$HOST --threshold=$LIGHTHOUSE_THRESHOLD --runs 3",
      "compodoc": "compodoc",
      "docker:login": "docker login -u $DOCKER_USER --password-stdin",
      "docker:build": "docker build -t samuelfernandez/portfolio .",
      "docker:run": "docker run --rm -it -p 5000:5000 -e PORT=5000 samuelfernandez/portfolio",
      "docker:local": "docker run -it -p 5000:5000 -e PORT=5000 -v \"%cd%\"/dist:/usr/src/app:ro samuelfernandez/portfolio sh",
      "docker:push": "docker push samuelfernandez/portfolio",
      "docker:compose": "npm run build && docker-compose up",
      "heroku:login": "heroku container:login",
      "heroku:deploy": "heroku container:push web --app $APP_NAME",
      "heroku:release": "heroku container:release web --app $APP_NAME",
      "semantic-release": "semantic-release"
   },
   "main": "dist/server.js",
   "dependencies": {
      "@angular/animations": "^7.1.0",
      "@angular/common": "^7.1.0",
      "@angular/compiler": "^7.1.0",
      "@angular/core": "^7.1.0",
      "@angular/forms": "^7.1.0",
      "@angular/http": "^7.1.0",
      "@angular/platform-browser": "^7.1.0",
      "@angular/platform-browser-dynamic": "^7.1.0",
      "@angular/platform-server": "^7.1.0",
      "@angular/pwa": "^0.11.0",
      "@angular/router": "^7.1.0",
      "@angular/service-worker": "^7.1.0",
      "@nguniversal/common": "^7.0.2",
      "@nguniversal/express-engine": "^7.0.2",
      "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
      "angulartics2": "^7.1.0",
      "apollo-server-express": "^2.2.6",
      "chalk": "^2.4.1",
      "cheerio": "^1.0.0-rc.2",
      "compression": "^1.7.3",
      "cookie-parser": "^1.4.3",
      "core-js": "^2.5.4",
      "cors": "^2.8.5",
      "dotenv": "^6.1.0",
      "express": "^4.16.3",
      "graphql": "^14.0.2",
      "helmet": "^3.15.0",
      "heroku-ssl-redirect": "^0.0.4",
      "is-heroku": "^2.0.0",
      "memory-cache": "^0.2.0",
      "mongodb": "^3.1.10",
      "mongoose": "^5.3.15",
      "morgan": "^1.9.1",
      "normalize.css": "^8.0.1",
      "path-to-regexp": "^2.4.0",
      "reflect-metadata": "^0.1.12",
      "request": "^2.88.0",
      "rxjs": "^6.3.3",
      "serve-favicon": "^2.5.0",
      "subscriptions-transport-ws": "^0.9.15",
      "zone.js": "^0.8.26"
   },
   "devDependencies": {
      "@angular-devkit/build-angular": "^0.11.0",
      "@angular/cli": "~7.1.0",
      "@angular/compiler-cli": "^7.1.0",
